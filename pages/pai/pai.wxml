<!-- 开始 -->

<view class='ul'>


    <block wx:for="{{songsList}}" wx:key="{{index}}" >
      <view class='li' bindtap='jumpListPage' id='{{item.id}}'>
      <view class='content-img'>
        <image src="{{item.pic}}" class='img'></image>
      </view>
      <view class='content-right'>
        <!-- right content -->
        <view class='right_cont'>
          <view class='title'>{{item.name}}</view>
          <view class='author'>创建：{{item.creator}}</view>
          <view class='author'>时间：{{item.createTime}}</view>
          <view class='author'>播放：{{item.playCount}}</view>
        </view>
      </view>
    </view>
    
    </block>

    

    
</view>